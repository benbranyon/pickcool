{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/benallfree/stapler"
      },
      {
          "type": "vcs",
          "url": "https://github.com/benallfree/ninja-mutex"
      }
  ],
  
  "require": {
    "laravel/framework": "4.2.*",
    "vlucas/phpdotenv": "1.0.*",
    "facebook/php-sdk-v4" : "4.0.*",
    "codesleeve/stapler": "dev-master as 1.0.0",
    "benallfree/laravel-fb-auth": "dev-master",
    "benallfree/laravel-stapler-images": "dev-master",
    "cocur/slugify": "1.*",
    "arvenil/ninja-mutex": "dev-master",
    "bugsnag/bugsnag-laravel": "1.*",
    "graham-campbell/markdown": "~2.0",
    "guzzlehttp/guzzle": "~5.0"
  },
  "autoload": {
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/models",
      "app/database/migrations",
      "app/tests/TestCase.php",
      "app/classes"
    ]
  },
  "minimum-stability": "stable"
}
