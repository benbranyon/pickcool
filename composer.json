{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/benallfree/stapler"
      },
      {
          "type": "vcs",
          "url": "https://github.com/benallfree/ninja-mutex"
      },
      {
          "type": "vcs",
          "url": "https://github.com/benallfree/dflydev-apache-mime-types"
      }
  ],
  
  "require": {
    "laravel/framework": "4.2.*",
    "vlucas/phpdotenv": "1.0.*",
    "facebook/php-sdk-v4" : "~5.0",
    "codesleeve/stapler": "dev-master as 1.0.0",
    "benallfree/laravel-stapler-images": "dev-master",
    "cocur/slugify": "1.*",
    "arvenil/ninja-mutex": "dev-master",
    "bugsnag/bugsnag-laravel": "1.*",
    "graham-campbell/markdown": "~2.0",
    "guzzlehttp/guzzle": "^6.1",
    "artdarek/oauth-4-laravel": "dev-master",
     "dflydev/apache-mime-types": "dev-master",
    "anahkiasen/flatten": "^0.5.1",
    "google/recaptcha": "^1.1"
  },
  "require-dev": {
    "panique/laravel-sass": "dev-master"
  },
  "autoload": {
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/models",
      "app/database/migrations",
      "app/tests/TestCase.php",
      "app/classes",
      "app/traits"
    ]
  },
  "minimum-stability": "stable"
}
